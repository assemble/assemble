<div class="examples">
  <h4>Casting data types</h4>
  <dl class="dl-horizontal">
    {{#dataTypes}}
      <dt>a.</dt> <dd>{{{a}}}</dd>
      <dt>b.</dt> <dd>{{{b}}}</dd>
      <dt>c.</dt> <dd>{{{c}}}</dd>
      <dt>d.</dt> <dd>{{{d}}}</dd>
      <dt>e.</dt> <dd>{{{e}}}</dd>
      <dt>f.</dt> <dd>{{{f}}}</dd>
      <dt>g.</dt> <dd>{{{g}}}</dd>
      <dt>h.</dt> <dd>{{{h}}}</dd>
    {{/dataTypes}}
  </dl>
</div>